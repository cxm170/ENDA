<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">




    <ListPreference
        android:title="Offloading File Path"
        android:key="listOffloadingFilePath"
        android:defaultValue="/sdcard/Pictures/input_small.jpg"
        android:entries="@array/listOffloadingFilePath"
        android:entryValues="@array/listOffloadingFilePath"
        />

    <ListPreference
        android:title="Fixed Delay"
        android:key="listFixedDelay"
        android:defaultValue="9"
        android:entries="@array/listFixedDelay"
        android:entryValues="@array/listFixedDelay"
        />

    <ListPreference
        android:title="Server Delay"
        android:key="listServerDelay"
        android:defaultValue="9"
        android:entries="@array/listServerDelay"
        android:entryValues="@array/listServerDelay"
        />


    <ListPreference
        android:title="Count"
        android:key="listCount"
        android:defaultValue="10"
        android:entries="@array/listCount"
        android:entryValues="@array/listCount"
        />

    <ListPreference
        android:title="HTTP Get URL"
        android:key="listHttpGet"
        android:defaultValue="http://lijiwei.tk:25004/facedetection/output/output_small.jpg"
        android:entries="@array/listHttpGet"
        android:entryValues="@array/listHttpGet"
        />

    <ListPreference
        android:title="Quality"
        android:key="listQuality"
        android:defaultValue="0.4"
        android:entries="@array/listQuality"
        android:entryValues="@array/listQuality"
        />



</PreferenceScreen>
